{
  "name": "@dlightjs/view-generator",
  "version": "1.0.0-alpha.0",
  "description": "DLight View Generator given different types of reactivity units",
  "author": {
    "name": "IanDx",
    "email": "iandxssxx@gmail.com"
  },
  "keywords": [
    "dlight.js"
  ],
  "license": "MIT",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsup --sourcemap",
    "test": "vitest",
    "test-ui": "vitest --ui"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/generator": "^7.23.6",
    "@dlightjs/view-parser": "workspace:*",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.6.8",
    "@types/node": "^20.10.5",
    "@vitest/ui": "^0.34.5",
    "tsup": "^6.7.0",
    "typescript": "^5.3.2",
    "vitest": "^0.34.5"
  },
  "dependencies": {
    "@dlightjs/error-handler": "workspace:*",
    "@dlightjs/reactivity-parser": "workspace:*"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "clean": true,
    "dts": true,
    "minify": true
  }
}
