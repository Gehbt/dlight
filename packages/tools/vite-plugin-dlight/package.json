{
  "name": "vite-plugin-dlight",
  "version": "0.10.6",
  "description": "DLight transpiler as vite plugin",
  "author": {
    "name": "IanDx",
    "email": "iandxssxx@gmail.com"
  },
  "keywords": [
    "dlight.js",
    "vite-plugin"
  ],
  "license": "MIT",
  "files": [
